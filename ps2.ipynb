    "#5\n",
    "import numpy as np\n",
    "a = np.array([[1,0,3],[1,2,1],[6,7,8]])\n",
    "r,c = a.shape\n",
    "x,y = 0,0\n",
    "for i in range(r):\n",
    "  for j in range(c):\n",
    "    if a[i][j]==0:\n",
    "      x = i\n",
    "      y = j\n",
    "for i in range(r):\n",
    "  for j in range(c):\n",
    "    if i==x or j==y:\n",
    "      a[i][j]= 0\n",
    "print(a)"
   ]
